root = true
# Editor configuration, see https://editorconfig.org

[*]
charset = utf-8
indent_size = 2
indent_style = space
end_of_line = lf
max_line_length = 10000
insert_final_newline = true
trim_trailing_whitespace = true

# JetBrains
ij_any_align_group_field_declarations = false
ij_any_block_comment_add_space = false
ij_any_block_comment_at_first_column = true
ij_any_keep_blank_lines_in_code = 1
ij_any_keep_indents_on_empty_lines = false
ij_any_keep_line_breaks = true
ij_any_line_comment_add_space = false
ij_any_line_comment_at_first_column = false
ij_any_space_after_colon = true
ij_any_space_after_comma = true
ij_any_space_before_colon = false
ij_any_space_before_comma = false
ij_any_spaces_around_assignment_operators = true
ij_any_spaces_within_braces = false
ij_any_spaces_within_brackets = false
ij_any_wrap_long_lines = false

# JetBrains CSS
ij_css_align_closing_brace_with_properties = false
ij_css_blank_lines_around_nested_selector = 1
ij_css_blank_lines_between_blocks = 1
ij_css_brace_placement = end_of_line
ij_css_enforce_quotes_on_format = false
ij_css_hex_color_long_format = false
ij_css_hex_color_lower_case = false
ij_css_hex_color_short_format = false
ij_css_hex_color_upper_case = false
ij_css_keep_single_line_blocks = false
